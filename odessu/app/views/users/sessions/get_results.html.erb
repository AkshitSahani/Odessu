<h2> Stores </h2>
<% if !@results[:store_results].empty? %>
  <% @results[:store_results].each do |store|%>
    <%= store %><br>
  <% end %>
<% else %>
  <h4> There were no stores found containing the term "<%= params[:search] %>".</h4>
<% end %>

<h2> Products </h2>
<% if !@results[:product_results].empty? %>
  <% @results[:product_results].each do |product| %>
    <%= product%><br>
  <% end %>
<% else %>
  <h4>There were no products found containing the term "<%= params[:search] %>".</h4>
<% end %>
